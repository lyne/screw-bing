{
    "manifest_version": 2,
    "name": "Screw Bing",
    "description": "Screw Bing and start using DuckDuckGo",
    "version": "0.1.1",
    "author": "lyne",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icon128x128.png"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://bing.com/*",
        "*://www.bing.com/*"
    ]
}