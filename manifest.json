{
    "manifest_version": 2,
    "name": "Screw Bing",
    "description": "Screw Bing and start using DuckDuckGo",
    "version": "0.1.0",
    "author": "lyne",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon128x128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.bing.com/search*"
            ],
            "run_at": "document_start",
            "js": [
                "redirect.js"
            ]
        }
    ]
}